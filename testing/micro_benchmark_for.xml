<?xml version="1.0"?>
<program>
    <variable>
        <name>sum</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <variable>
        <name>sum1</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <for>
        <init>
            <variable>
                <name>i</name>
                <type>int</type>
                <value>0</value>
            </variable>
            <const>5</const>
        </init>
        <cond>
            <expression>
                <operator>&lt;</operator>
                <operand>
                    <label>i</label>
                </operand>
                <operand>
                    <label>for_0_constant</label>
                </operand>
            </expression>
        </cond>
        <body>
            <statement>
                <lhs>
                    <label>sum</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>sum</label>
                        </operand>
                        <operand>
                            <label>i</label>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </body>
        <increment>
            <statement>
                <lhs>
                    <label>i</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>i</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </increment>
        <exit>

        </exit>
    </for>
    <for>
        <init>
            <variable>
                <name>k</name>
                <type>int</type>
                <value>10</value>
            </variable>
            <const>5</const>
        </init>
        <cond>
            <expression>
                <operator>&gt;</operator>
                <operand>
                    <label>k</label>
                </operand>
                <operand>
                    <label>for_1_constant</label>
                </operand>
            </expression>
        </cond>
        <body>
            <statement>
                <lhs>
                    <label>sum1</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>sum1</label>
                        </operand>
                        <operand>
                            <label>k</label>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </body>
        <increment>
            <statement>
                <lhs>
                    <label>k</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>-</operator>
                        <operand>
                            <label>k</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </increment>
        <exit>

        </exit>
    </for>
</program>