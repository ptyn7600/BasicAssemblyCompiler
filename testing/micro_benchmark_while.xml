<?xml version="1.0"?>
<program>
    <variable>
        <name>i</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <variable>
        <name>k</name>
        <type>int</type>
        <value>10</value>
    </variable>
    <variable>
        <name>sum</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <variable>
        <name>sum1</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <while>
        <init>
            <const>5</const>
        </init>
        <cond>
            <expression>
                <operator>&lt;</operator>
                <operand>
                    <label>i</label>
                </operand>
                <operand>
                    <label>while_0_constant</label>
                </operand>
            </expression>
        </cond>
        <body>
            <statement>
                <lhs>
                    <label>sum</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>sum</label>
                        </operand>
                        <operand>
                            <label>i</label>
                        </operand>
                    </expression>
                </rhs>
            </statement>
            <statement>
                <lhs>
                    <label>i</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>i</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </body>
        <exit>

        </exit>
    </while>
    <while>
        <init>
            <const>5</const>
        </init>
        <cond>
            <expression>
                <operator>&gt;</operator>
                <operand>
                    <label>k</label>
                </operand>
                <operand>
                    <label>while_1_constant</label>
                </operand>
            </expression>
        </cond>
        <body>
            <statement>
                <lhs>
                    <label>sum1</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>sum1</label>
                        </operand>
                        <operand>
                            <label>k</label>
                        </operand>
                    </expression>
                </rhs>
            </statement>
            <statement>
                <lhs>
                    <label>k</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>-</operator>
                        <operand>
                            <label>k</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </body>
        <exit>

        </exit>
    </while>
</program>