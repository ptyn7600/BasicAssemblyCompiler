<?xml version="1.0"?>
<program>
    <variable>
        <name>a</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <variable>
        <name>b</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <variable>
        <name>i</name>
        <type>int</type>
        <value>0</value>
    </variable>
    <if>
        <expression>
            <operator>==</operator>
            <operand>
                <label>a</label>
            </operand>
            <operand>
                <label>b</label>
            </operand>
        </expression>
        <true>
            <statement>
                <lhs>
                    <label>a</label>
                </lhs>
                <rhs>
                    <const>7</const>
                </rhs>
            </statement>
        </true>
        <false>
            <statement>
                <lhs>
                    <label>a</label>
                </lhs>
                <rhs>
                    <const>8</const>
                </rhs>
            </statement>
        </false>
    </if>
    <while>
        <init>
            <const>10</const>
        </init>
        <cond>
            <expression>
                <operator>&lt;</operator>
                <operand>
                    <label>i</label>
                </operand>
                <operand>
                    <label>while_0_constant</label>
                </operand>
            </expression>
        </cond>
        <body>
            <statement>
                <lhs>
                    <label>a</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>a</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
            <statement>
                <lhs>
                    <label>i</label>
                </lhs>
                <rhs>
                    <expression>
                        <operator>+</operator>
                        <operand>
                            <label>i</label>
                        </operand>
                        <operand>
                            <const>1</const>
                        </operand>
                    </expression>
                </rhs>
            </statement>
        </body>
        <exit>

        </exit>
    </while>
</program>